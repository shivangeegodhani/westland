.btns{ width:100%; max-width:260px; padding:15px 20px 16px; margin:0; display:inline-block; font-size:18px; color:#ffffff !important; text-transform:uppercase; text-decoration: none; letter-spacing:1.5px; font-weight:700; border:none; background:#000000 !important; -webkit-transform: perspective(1px) translateZ(0); transform: perspective(1px) translateZ(0);}
