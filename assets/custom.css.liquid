/* .btns::before{ content: ''; position: absolute; border: #4f4f4f solid 4px; top: 0; right: 0; bottom: 0; left: 0; -webkit-animation-duration: 1s; animation-duration: 1s; opacity:0;} */
.btns:hover{ color:#fff !important; background:#4f4f4f !important;}
.btns:focus{ color:#fff; background:#000000;}
.btns:hover::before { -webkit-animation-name: hvr-ripple-out; animation-name: hvr-ripple-out; opacity:1;}

/* Ripple Out */
@-webkit-keyframes hvr-ripple-out {
    100% {
        top: -12px;
        right: -12px;
        bottom: -12px;
        left: -12px;
        opacity: 0;
    }
}
@keyframes hvr-ripple-out {
    100% {
        top: -12px;
        right: -12px;
        bottom: -12px;
        left: -12px;
        opacity: 0;
    }
}

